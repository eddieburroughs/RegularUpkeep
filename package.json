{
  "name": "regularupkeep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run",
    "test:integration": "vitest run",
    "test:e2e": "echo 'E2E tests not configured (Playwright/Cypress). Skipping.' && exit 0",
    "test:coverage": "vitest run --coverage",
    "test:security": "npm audit --audit-level=high && ./scripts/security-check.sh",
    "test:rls": "vitest run --config vitest.security.config.ts",
    "test:security:rls": "npm run test:rls",
    "test:all": "node scripts/test-all.mjs",
    "test:ci": "CI=true NEXT_TELEMETRY_DISABLED=1 node scripts/test-all.mjs",
    "security-check": "./scripts/security-check.sh",
    "audit": "npm audit",
    "check-all": "npm run lint && npm run typecheck && npm run test:run && npm run audit",
    "kb:import": "npx ts-node scripts/kb-import.ts"
  },
  "dependencies": {
