# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://api.regularupkeep.com
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Configuration (Hybrid Mode)
# OpenAI: Used for vision tasks (photo analysis, intake classification)
OPENAI_API_KEY=sk-proj-your-openai-key-here

# Anthropic: Used for long-form text (estimates, messages, maintenance plans)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Email Notifications (Resend)
RESEND_API_KEY=re_your-resend-key-here

# Cron Job Authentication
CRON_SECRET=your-cron-secret-here

# Web Push Notifications (VAPID)
# Generate with: npx web-push generate-vapid-keys
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key-here
VAPID_PRIVATE_KEY=your-vapid-private-key-here

# SMS Notifications (Twilio)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Google Maps APIs (for Provider Discovery)
# Enable: Places API, Geocoding API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
